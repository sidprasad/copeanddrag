{
  "name": "cope-and-drag",
  "version": "1.0.1",
  "description": "A web application for lightweight diagramming for Alloy/Forge.",
  "main": "dist/index.js",
  "keywords": [],
  "author": "Siddhartha Prasad",
  "license": "ISC",
  "devDependencies": {
    "@types/firebase": "^2.4.32",
    "@types/node": "^20.14.12",
    "cpx": "^1.5.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && ncc build src/index.ts -o dist && npm run copy-files",
    "copy-files": "cpx \"views/**/*\" dist/views && cpx \"public/**/*\" dist/public",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/graphlib": "^2.1.12",
    "@types/webcola": "^3.2.0",
    "cassowary": "^0.0.2",
    "chroma-js": "^3.1.1",
    "cytoscape": "^3.30.1",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "express": "^4.18.2",
    "firebase": "^11.2.0",
    "graphlib": "^2.1.8",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.1.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "vector2": "^0.1.1",
    "webcola": "^3.4.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "public/**/*",
    "views/**/*",
    "package.json",
    "README.md"
  ]
}
